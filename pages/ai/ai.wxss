/* pages/ai/ai.wxss */
.table {
  display: flex;
  flex-direction: column;     /* 排列形式: 向下 */
  margin-bottom: 50px;
}

.tr {
  width: 210%;
  display: flex;
  flex-direction: row;
}

.th_1, .th_2, .th_3, .td_1, .td_2, .td_3 { /* 公有的属性 */
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;            /* 自动换行 */
  text-align: center;         /* 文本居中 */
  justify-content:center;     /* 主轴居中 */
  align-items: center;        /* 交叉轴居中 */
  border-top: 3px solid #dadada;      /* 单元格上线框 */
  border-bottom: 3px solid #dadada;   /* 单元格下线框 */
  border-left: 3px solid #dadada;     /* 单元格左线框 */
  border-right: 3px solid #dadada;    /* 单元格右线框 */
}



.td_1, .td_2, .td_3{
  height:70px;
}

.th_1, .td_1 {
  width: 15%;
}

.th_2, .td_2 {
  width: 15%;
}

.th_3, .td_3 {
  width: 15%;
}
.container{
  width: 100%;
  height: 600rpx;
  margin: 0;
  padding: 0;
  position: relative;
}
image{
  position: absolute;
  width: 100%;
  height: 1500rpx;
}
.content{
  position: absolute;
  z-index: 1;
}


